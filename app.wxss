/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 300rpx 0;
  box-sizing: border-box;
  color: white;
  width: 100%;
}

page {
  background-color: #dcdcdc;
}

.juzhao {
  width: 90%;
  height: 310rpx;
}

.grade {
  display: flex;
  flex-direction: row;
  font-size: 12px;
}

.star {
  width: 23rpx;
  height: 23rpx;
}

.textview {
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 12px;
}

.back {
  position: relative;
  z-index: -1;
  width: 60%;
  top: -23rpx;
  height: 16rpx;
  background-color: orange;
}
